{
  "name": "react-fcm-playground",
  "description": "A playground for Firebase Cloud Messaging (FCM) with React",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">= 20.15",
    "pnpm": ">=10.6.3"
  },
  "scripts": {
    "dev": "pnpm run prebuild && vite",
    "prebuild": "node scripts/generate-fcm-sw.js",
    "prebuild:local": "dotenvx run -- node scripts/generate-fcm-sw.js",
    "build": "tsc -b && vite build",
    "build:local": "tsc -b && vite build",
    "lint": "eslint .",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,mjs,ts,tsx,css,scss,md,json}\"",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.19.2",
    "@emotion/react": "^11.14.0",
    "firebase": "^11.8.1",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@dotenvx/dotenvx": "^1.44.1",
    "@eslint/js": "^9.19.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.85.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "vitest-axe": "^0.1.0"
  }
}
